{
	"_version": "1.12.0",
	"sap.app": {
		"id": "webapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0",
			"bundleLocale": "ru"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"emplData": {
				"uri": "http://sapsb8.mow.molga.ru:8000/sap/opu/odata/SAP/ZMS_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
	"sap.ui5": {
		"rootView": {
			"viewName": "webapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "webapp.i18n.i18n"
				}
			},
			"empl": {
				"dataSource": "emplData"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "webapp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": ":?query:",
					"name": "home",
					"target": ["home"]
				}, {
					"pattern": "persons/{Pernr}",
					"name": "persons",
					"target": "persons"
				}
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewId": "home",
					"viewLevel": 1,
					"title": "{i18n>title}"
				},
				"notFound": {
				   "viewId": "notFound",
				   "viewName": "NotFound",
				   "transition": "show"
				},
				"persons": {
					"viewId": "persons",
					"viewName": "persons.Persons",
					"viewLevel" : 3
				}
			}
		}
	}
}